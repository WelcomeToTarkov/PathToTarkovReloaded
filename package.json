{
  "name": "path-to-tarkov-reloaded",
  "displayName": "Path To Tarkov Reloaded",
  "fullName": "Trap-RockaHorse-PathToTarkov-Reloaded",
  "version": "5.2.6",
  "main": "src/mod.js",
  "license": "MIT",
  "author": "Trap.RockaHorse.WTT",
  "akiVersion": "3.8.x",
  "scripts": {
    "gather:external:resources": "node ./scripts/gather-external-resources.js && npm run prettier",
    "build:doc:all-exfils": "node scripts/generate-all-exfils > ALL_EXFILS.md",
    "dev:doc": "node scripts/generate-all-exfils",
    "build:doc": "npm run build:doc:all-exfils",
    "zip:files": "cd dist && bestzip ../mod.zip user BepInEx && cd .. && node scripts/rename-zip.js",
    "prepare:files": "node scripts/prepare-files.js",
    "build:release": "npm run lint:all && npm run rebuild:all && npm run prepare:files && npm run zip:files",
    "clean": "rimraf dist tmp 'src/**/*.js' Trap-PathToTarkov*",
    "clean:all": "npm run clean:client && npm run clean",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "lint": "eslint --ignore-pattern '*.js' .",
    "lint:all": "npm run prettier:check && npm run lint",
    "rebuild:all": "npm run clean:all && npm run build:all",
    "rebuild": "npm run clean && npm run build",
    "build": "tsc",
    "build:all": "npm run build:doc && npm run build && npm run build:client",
    "build:client": "cd PTT-Extracts && dotnet.exe build",
    "clean:client": "rimraf PTT-Extracts/bin PTT-Extracts/obj",
    "test:all": "npm run build:release",
    "test": "tsc --noEmit && npm run lint:all"
  },
  "devDependencies": {
    "@types/i18n": "^0.13.5",
    "@types/node": "^14.15.3",
    "@types/winston": "^2.4.4",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "bestzip": "2.2.1",
    "eslint": "7.26.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tsyringe": "4.6.0",
    "typescript": "4.6.4",
    "winston": "^3.7.2"
  }
}
